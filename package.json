  },
  "license": "ISC",
  "private": false,
  "type": "module",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "npm run clean && npm run build:types && npm run build:lib",
    "build:types": "tsc --emitDeclarationOnly --noEmit false --declaration true",
    "build:lib": "NODE_ENV='production' vite build --mode production",
    "build:storybook": "storybook build",
    "lint": "eslint --config eslint.config.mjs",
    "test": "vitest run",
    "preversion": "npm run lint && npm test",
    "postversion": "git push && git push --tags",
    "storybook": "storybook dev --exact-port --port 6060 --disable-telemetry --no-open"
  },
  "peerDependencies": {
    "react": "^17 || ^18 || ^19",
    "react-dom": "^17 || ^18 || ^19"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@rollup/plugin-replace": "^6.0.1",
    "@storybook/addon-controls": "^8.4.4",
    "@storybook/addon-essentials": "^8.4.4",
    "@storybook/addon-interactions": "^8.4.4",
    "@storybook/addon-links": "^8.4.4",
    "@storybook/addon-themes": "^8.4.4",
    "@storybook/blocks": "^8.4.4",
    "@storybook/components": "^8.4.4",
    "@storybook/icons": "^1.2.12",
    "@storybook/manager-api": "^8.4.4",
    "@storybook/react": "^8.4.4",
    "@storybook/react-vite": "^8.4.4",
    "@storybook/test": "^8.4.4",
    "@storybook/theming": "^8.4.4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.0.1",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/parser": "^8.15.0",
    "@typescript-eslint/typescript-estree": "^8.15.0",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "eslint": "^9.15.0",
    "eslint-plugin-react": "^7.37.2",
    "glob": "^11.0.0",
    "globals": "^15.12.0",
    "jsdom": "^25.0.1",
    "react-hook-form": "^7.53.2",
    "sass-embedded": "^1.81.0",
    "storybook": "^8.4.4",
    "tsx": "^4.19.2",
    "typescript": "~5.5.4",
    "typescript-eslint": "^8.15.0",
    "vite": "^6.0.0",
    "vite-plugin-cp": "^4.0.8",
    "vite-plugin-lib-inject-css": "^2.1.1",
    "vite-plugin-watch-and-run": "^1.7.1",
    "vitest": "^2.1.5"
  },
  "overrides": {
    "glob": "^11.0.0",
    "cookie": "^0.7.0",
    "@storybook/builder-vite": {
      "express": "^4.21.0"
    },
    "express": {
      ".": "^4.21.0"
    },
    "jsonfile": {
      ".": "^6.1.0"
    },
    "globals": {
      ".": "^15.12.0"
    }
  }
