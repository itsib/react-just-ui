{
  "name": "react-just-ui",
  "description": "UI components library for react.",
  "version": "0.3.7",
  "author": "Sergey <itsib.su@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itsib/react-just-ui.git"
  },
  "license": "ISC",
  "private": false,
  "type": "module",
  "scripts": {
    "watch": "vite watch",
    "clean": "rm -rf ./dist",
    "build": "npm run clean && npm run build:types && npm run build:lib",
    "build:types": "tsc --emitDeclarationOnly --noEmit false --declaration true",
    "build:lib": "NODE_ENV='production' vite build --mode production",
    "build:storybook": "storybook build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest run",
    "preversion": "npm run lint",
    "postversion": "git push && git push --tags",
    "storybook": "storybook dev --port 6006 --disable-telemetry --no-open"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^2.0.2",
    "@storybook/addon-controls": "8.3.1",
    "@storybook/addon-essentials": "8.3.1",
    "@storybook/addon-interactions": "8.3.1",
    "@storybook/addon-links": "8.3.1",
    "@storybook/addon-themes": "8.3.1",
    "@storybook/blocks": "8.3.1",
    "@storybook/components": "8.3.1",
    "@storybook/icons": "^1.2.12",
    "@storybook/manager-api": "8.3.1",
    "@storybook/react": "8.3.1",
    "@storybook/react-vite": "8.3.1",
    "@storybook/test": "8.3.1",
    "@storybook/theming": "8.3.1",
    "@types/node": "^20.14.12",
    "@types/postcss-advanced-variables": "^3.0.0",
    "@types/postcss-import": "^14.0.3",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "autoprefixer": "^10.4.19",
    "esbuild": "^0.24.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-storybook": "^0.8.0",
    "glob": "^11.0.0",
    "postcss": "^8.4.47",
    "postcss-advanced-variables": "^4.0.0",
    "postcss-import": "^16.1.0",
    "postcss-nesting": "^12.1.5",
    "react-just-ui": "file:./dist",
    "storybook": "8.3.1",
    "tsx": "^4.19.1",
    "typescript": "^5.5.4",
    "vite": "^5.4.8",
    "vite-plugin-cp": "^4.0.8",
    "vite-plugin-lib-inject-css": "^2.1.1",
    "vitest": "^2.1.1"
  },
  "overrides": {
    "glob": "^11.0.0",
    "rollup": "^4.22.4",
    "storybook": "8.3.1",
    "@storybook/components": {
      ".": "8.3.1"
    },
    "@storybook/react": {
      ".": "8.3.1",
      "@storybook/components": "8.3.1",
      "@storybook/preview-api": "8.3.1"
    },
    "jsonfile": {
      ".": "^6.1.0"
    }
  }
}
