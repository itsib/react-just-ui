{
  "name": "react-just-ui",
  "description": "UI components library for react.",
  "version": "0.5.14",
  "author": {
    "name": "Sergey",
    "email": "itsib.su@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itsib/react-just-ui.git"
  },
  "license": "ISC",
  "private": false,
  "type": "module",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "npm run clean && npm run build:types && npm run build:lib",
    "build:types": "tsc --emitDeclarationOnly --noEmit false --declaration true",
    "build:lib": "NODE_ENV='production' vite build --mode production",
    "build:storybook": "storybook build",
    "lint": "eslint --config eslint.config.mjs",
    "test": "vitest run",
    "preversion": "npm run lint && npm test",
    "postversion": "git push && git push --tags",
    "storybook": "storybook dev --exact-port --port 6060 --disable-telemetry --no-open",
    "chromatic": "chromatic --build-script-name=build:storybook --exit-zero-on-changes"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.0.0",
    "@eslint/js": "^9.11.1",
    "@rollup/plugin-replace": "^6.0.1",
    "@storybook/addon-controls": "^8.3.4",
    "@storybook/addon-essentials": "^8.3.4",
    "@storybook/addon-interactions": "^8.3.4",
    "@storybook/addon-links": "^8.3.4",
    "@storybook/addon-themes": "^8.3.4",
    "@storybook/blocks": "^8.3.4",
    "@storybook/components": "^8.3.4",
    "@storybook/icons": "^1.2.12",
    "@storybook/manager-api": "^8.3.4",
    "@storybook/react": "^8.3.4",
    "@storybook/react-vite": "^8.3.4",
    "@storybook/test": "^8.3.4",
    "@storybook/theming": "^8.3.4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.0.1",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/parser": "^8.8.1",
    "@typescript-eslint/typescript-estree": "^8.9.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "eslint": "^9.11.1",
    "glob": "^11.0.0",
    "globals": "^15.11.0",
    "jsdom": "^25.0.1",
    "react-hook-form": "^7.53.0",
    "sass-embedded": "^1.79.4",
    "storybook": "^8.3.4",
    "tsx": "^4.19.1",
    "typescript": "5.5",
    "typescript-eslint": "^8.8.1",
    "vite": "^5.4.8",
    "vite-plugin-cp": "^4.0.8",
    "vite-plugin-lib-inject-css": "^2.1.1",
    "vite-plugin-watch-and-run": "^1.7.1",
    "vitest": "^2.1.1"
  },
  "overrides": {
    "glob": "^11.0.0",
    "cookie": "^0.7.0",
    "@storybook/builder-vite": {
      "express": "^4.21.0"
    },
    "express": {
      ".": "^4.21.0"
    },
    "jsonfile": {
      ".": "^6.1.0"
    },
    "globals": {
      ".": "^15.11.0"
    }
  },
  "bugs": {
    "url": "https://github.com/itsib/react-just-ui/issues"
  },
  "homepage": "https://github.com/itsib/react-just-ui#readme"
}
