{
  "extends": "tsconfig.json",
  "compilerOptions": {
    "rootDir": ".",
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowJs": true
  },
  "include": [
    "vite.config.ts",
    "plugins"
  ]
}